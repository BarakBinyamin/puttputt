{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../../../view/dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "statusdashboard",
    "version": "0.0.0"
  },
   "tauri": {
    "allowlist": {
      "all": true,
      "shell": {
        "sidecar": true,
        "scope": [
          {
            "name": "../../../backend/index3",
            "sidecar": true
          }
        ]
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.status-dashboard.dev",
      "deb": {
        "depends": []
      },
      "externalBin": ["../../../backend/index3"],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "statusdashboard",
        "width": 800,
        "height": 600
      }
    ]
  }
}
